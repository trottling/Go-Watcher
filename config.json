{
  "Proxy_Server": {
    "Port": 8080,
    "Show_Connections_STDOUT": true
  },
  "Activity_Handler": {
    "Non_legit_Ports_RPM": 10,
    "Legit_Ports_RPM": 50,
    "Legit_Paths_Ignore_Regex": [".*/favicon.ico$"],
    "Legit_Ports": [80, 443],
    "Block_IPs": true,
    "Block_IPs_time": 600,
    "Dump_Requests": true,
    "Requests_Dump_Ignore_Regex": [".*.ico$", ".*.css$", ".*.png$", ".*.js$"]
  }
}
